{
    "name": "sailingdeveloper/eloquent-sortable",
    "description": "Sortable behaviour for eloquent models",
    "homepage": "https://github.com/spatie/eloquent-sortable",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        }
    ],
    "keywords":
        [
            "sort",
            "sortable",
            "eloquent",
            "model",
            "laravel",
            "behaviour"
        ],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "illuminate/database": "~5.3.0|~5.4.0|5.5.*-dev",
        "illuminate/support": "~5.3.0|~5.4.0|5.5.*-dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0",
        "orchestra/testbench": "~3.3.0|~3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\EloquentSortable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\EloquentSortable\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
